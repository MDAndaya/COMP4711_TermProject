
<div class="disc-title">Latest discussions</div>

<div class="disc-list">
    {{#each artists}}
        <div class="card">
            <img class="photo" src={{this.url}} alt="p1">
            <h3 class="name">{{this.name}}</h3>
            <p class="education">{{this.about}}</p>
            <form method="post" action="/artist/delete/{{this.id}}">
                <input type="submit" value="Delete" class="delete-btn">
            </form>
        </div> 
    {{/each}}
</div>